{
  "defaultSeverity": "warn",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-var-requires": [false]
  },
  "rules": {
    "interface-name": [false],
    "no-console": [false],
    "trailing-comma": [false],
    "quotemark": [true, "single"],
    "only-arrow-functions": [false],
    "object-literal-sort-keys": [false],
    "max-line-length": [false]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["bin/**"]
  }
}